{
    "extends": [
        "airbnb",
        "prettier",
        "prettier/flowtype",
        "prettier/react",
        "plugin:react-native/all"
    ],
    "parser": "babel-eslint",
    "plugins": [
        "react-native",
        "flow"
    ],
    "rules": {
        "react-native/no-color-literals": "off",
        "react/jsx-filename-extension": "off",
        "global-require": "off"
    }
}
